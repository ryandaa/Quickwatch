{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html",
        "function": "quickwatchv1"
      }
    ],
    "predeploy": [
      "npm run build"
    ]
  }
}
